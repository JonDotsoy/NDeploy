  },
  "bin": {
    "ndeploy": "bin/cli.js"
  },
  "author": "Jon Dotsoy <hi@jon.soy> (http://jon.soy)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonDotsoy/ndeploy.git"
  },
  "bugs": {
    "url": "https://github.com/JonDotsoy/ndeploy/issues"
  },
  "homepage": "https://github.com/JonDotsoy/ndeploy#readme",
  "description": "A tool for deploying an application.",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
